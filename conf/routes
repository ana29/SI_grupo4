# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message

GET     /home                       controllers.HomeController.chamarHome

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

POST /cadastraUsuario               controllers.HomeController.cadastrarUsuario()
POST /logar                         controllers.HomeController.logar()

GET /Login                          controllers.HomeController.chamarLogin()
GET /Cadastro                       controllers.HomeController.chamarCadastro()
GET /usuario                        controllers.HomeController.menuUsuario()
GET /logOut                         controllers.HomeController.logOut()
GET /criaPasta                      controllers.HomeController.criaPasta()
GET /Caixa                          controllers.HomeController.chamarCaixa()

#POST /salvaArquivo                  controllers.HomeController.salvaArquivo()

GET /criaArquivos                   controllers.HomeController.criaArquivos()
GET /chamaTexto                     controllers.HomeController.chamaTexto()